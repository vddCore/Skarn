<Styles x:Class="Typhoon.UI.BreezeTheme"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="Colors/BaseColors.axaml" />
                <ResourceInclude Source="Colors/BaseBrushes.axaml" />
                
                <MergeResourceInclude Source="Resources/BaseResources.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Styles.Resources>
    
    <StyleInclude Source="Controls.axaml" />
</Styles>
